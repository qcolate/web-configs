{
  "name": "@qcolate/eslint-config",
  "version": "1.3.0",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qcolate/web-configs.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/qcolate/web-configs/issues"
  },
  "scripts": {
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0 -s",
    "npm-publish": "git push --follow-tags origin master && npm publish",
    "versionup": "standard-version -r patch",
    "versionup:minor": "standard-version -r minor",
    "versionup:major": "standard-version -r major",
    "release": "yarn publish-npm",
    "publish-npm": "npm publish --access public"
  },
  "keywords": [
    "eslint",
    "airbnb",
    "config",
    "eslintconfig",
    "javascript",
    "styleguide",
    "typescript"
  ],
  "dependencies": {
    "eslint-config-airbnb-typescript": "16.0.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "19.0.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.2.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.4.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
