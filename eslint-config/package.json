{
  "name": "@qcolate/eslint-config",
  "description": "Qcolate Eslint Config",
  "version": "1.4.2",
  "license": "MIT",
  "author": "Jason Feng <solidzoro@live.com>",
  "main": "index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qcolate/web-configs.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/qcolate/web-configs/issues"
  },
  "scripts": {
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0 -s",
    "npm-publish": "git push --follow-tags origin master && npm publish",
    "versionup": "standard-version -r patch -t @qcolate/eslint-config\\@",
    "versionup:minor": "standard-version -r minor -t @qcolate/eslint-config\\@",
    "versionup:major": "standard-version -r major -t @qcolate/eslint-config\\@",
    "release": "yarn publish-npm",
    "publish-npm": "npm publish --access public"
  },
  "keywords": [
    "eslint",
    "airbnb",
    "config",
    "eslintconfig",
    "javascript",
    "styleguide",
    "typescript"
  ],
  "updd": {
    "client": "yarn",
    "lockDeps": {},
    "lockDevDeps": {},
    "---- MEMO ----": {
      "lockDeps": {},
      "lockDevDeps": {}
    }
  },
  "peerDependencies": {
    "eslint-config-airbnb-typescript": "^17.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
