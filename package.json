{
  "name": "qcolate-web-configs",
  "author": "Jason Feng <solidzoro@live.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "versionup": "lerna version patch --conventional-commits",
    "versionup:minor": "lerna version minor --conventional-commits",
    "versionup:major": "lerna version major --conventional-commits",
    "linkall": "lerna exec -- yarn link",
    "release-babel": "cd ./packages/babel-preset && npm publish --access=public",
    "release-eslint": "cd ./packages/eslint-config && npm publish --access=public",
    "release-all": "yarn release-babel && yarn release-eslint",
    "release": "lerna publish --skip-npm && lerna run publish-npm"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  },
  "dependencies": {}
}
